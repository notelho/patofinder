{
  "name": "patofinder",
  "version": "1.0.0-rc.1",
  "description": "An elegant way to search for urls with web scraping",
  "author": "Nathan Botelho <nimbotelho@gmail.com> (https://github.com/notelho)",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:umd",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:umd": "webpack",
    "test": "npm run test:build && npm run test:cli",
    "test:cli": "npm i -g . && patofinder",
    "test:build": "npm run build:cjs && node ./lib/index.js"
  },
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.js",
  "bin": {
    "patofinder": "lib/bin/patofinder.js"
  },
  "homepage": "https://www.npmjs.com/package/patofinder",
  "bugs": {
    "url": "https://github.com/notelho/patofinder/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/notelho/patofinder"
  },
  "files": [
    "lib",
    "esm",
    "umd"
  ],
  "keywords": [
    "path searcher",
    "path finder",
    "url searcher",
    "url finder",
    "web scraping",
    "data mining",
    "path",
    "url"
  ],
  "dependencies": {
    "axios": "^0.19.2",
    "chalk": "^4.0.0",
    "clear": "^0.1.0",
    "commander": "^5.1.0",
    "ffprobe": "^1.1.0",
    "ffprobe-static": "^3.0.0",
    "typescript": "^3.9.2"
  },
  "devDependencies": {
    "@babel/parser": "^7.9.6",
    "@babel/types": "^7.9.6",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/node": "^14.0.1",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "commitizen": "^4.1.2",
    "eslint": "^7.0.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "pretty-format": "^26.0.1",
    "ts-loader": "^7.0.4",
    "tslint": "^6.1.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "peerDependencies": {}
}
