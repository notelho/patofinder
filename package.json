{
  "name": "patofinder",
  "version": "0.2.0",
  "description": "An elegant way to search for url types on different html pages",
  "author": "Nathan Botelho <nimbotelho@gmail.com> (https://github.com/notelho)",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.js",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:umd",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:umd": "webpack",
    "test": "npm run build:cjs && node ./lib/index.js"
  },
  "bin": {
    "patofinder": "./bin/bin.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/notelho/patofinder"
  },
  "files": [
    "lib",
    "esm",
    "umd"
  ],
  "keywords": [
    "patofinder",
    "pathfinder",
    "pathresolver",
    "path",
    "finder"
  ],
  "bugs": {
    "url": "https://github.com/notelho/patofinder/issues"
  },
  "homepage": "https://github.com/notelho/patofinder#readme",
  "peerDependencies": {},
  "devDependencies": {
    "@babel/parser": "^7.9.6",
    "@babel/types": "^7.9.6",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/node": "^14.0.1",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "commitizen": "^4.1.2",
    "eslint": "^7.0.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "pretty-format": "^26.0.1",
    "ts-loader": "^7.0.4",
    "tslint": "^6.1.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "ffprobe": "^1.1.0",
    "ffprobe-static": "^3.0.0",
    "typescript": "^3.9.2"
  }
